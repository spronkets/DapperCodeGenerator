{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "DapperCodeGenerator.Web",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/DapperCodeGenerator.Web/bin/Debug/net9.0/DapperCodeGenerator.Web.dll",
      "args": [],
      "cwd": "${workspaceFolder}/DapperCodeGenerator.Web",
      "console": "internalConsole",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:60420;http://localhost:60421"
      }
    }
  ]
}